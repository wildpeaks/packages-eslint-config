{
	"compilerOptions": {
		"moduleResolution": "node",
		"newLine": "LF",
		"alwaysStrict": true,
		"noEmitOnError": true,
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"noImplicitThis": true,
		"strictNullChecks": true,
		"preserveConstEnums": true,
		"removeComments": true,
		"sourceMap": true,
		"module": "commonjs",
		"target": "es5",
		"lib": ["es2017"]
	}
}
